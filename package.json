{
  "name": "boba",
  "version": "1.0.0",
  "description": "A local php development server with custom built panel.",
  "scripts": {
    "build"  : "cd ./Boba/ && go build && cd ../",
    "compile": "cd ./Boba/ && go build Boba.go && cd ../",
    "start"    : "cd ./Boba/ && go run Boba.go && cd ../",

    "set-up" : "cd ./scripts/ && node cli.js -env && cd ../",
    "deploy" : "cd ./scripts/ && node cli.js -d && cd ../",
    "update" : "cd ./scripts/ && node cli.js -upgrade && cd ../"
  },
  "keywords": [],
  "author": "Fernando Camblor",
  "license": "ISC"
}
